	,"IdenDoc_"."IdenDoc_Name" "IdenDoc_IdenDoc_Name"
	,"Base_Customer"."Identity_ID" "Identity_ID"
	,"Base_Customer"."tName_Locale" "tName_Locale"
	,"Base_Customer"."fName_Locale" "fName_Locale"
