    <field name="KioskOwner_Branch_KioskOwner_Branch_Code" type="String" readOnly="true" label="Kiosk Owner Branch Code" length="10" />
    <field name="Item_Flag" type="Boolean" default="((1))" label="Item Flag" hidden="true" />
