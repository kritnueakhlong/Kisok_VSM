  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[select
	"M_Kiosk_Material"."Kiosk_ID" "Kiosk_ID"
	,"Kiosk_"."Kiosk_Code" "Kiosk_Kiosk_Code"
	,"Kiosk_KioskType_"."KioskType_Name" "Kiosk_KioskType_KioskType_Name"
	,"Kiosk_Location_"."Location_Address" "Kiosk_Location_Location_Address"
	,"Kiosk_Location_KioskOwner_"."KioskOwner_Code" "Kiosk_Location_KioskOwner_KioskOwner_Code"
	,"M_Kiosk_Material"."Mater_ID" "Mater_ID"
	,"Mater_"."MaterType_Code" "Mater_MaterType_Code"
	,"Mater_MaterType_"."MaterType_Name" "Mater_MaterType_MaterType_Name"
	,"M_Kiosk_Material"."CreatedBy" "CreatedBy"
	,"M_Kiosk_Material"."CreatedOn" "CreatedOn"
	,"M_Kiosk_Material"."ModifiedBy" "ModifiedBy"
	,"M_Kiosk_Material"."ModifiedOn" "ModifiedOn"
from "dbo"."M_Kiosk_Material" "M_Kiosk_Material"
	left join "dbo"."M_Kiosk" "Kiosk_" on "M_Kiosk_Material"."Kiosk_ID" = "Kiosk_"."Kiosk_ID"
	left join "dbo"."M_Kiosk_Type" "Kiosk_KioskType_" on "Kiosk_"."KioskType_Code" = "Kiosk_KioskType_"."KioskType_Code"
	left join "dbo"."M_KioskOwner_Location" "Kiosk_Location_" on "Kiosk_"."Location_ID" = "Kiosk_Location_"."Location_ID"
	left join "dbo"."M_Kiosk_Owner" "Kiosk_Location_KioskOwner_" on "Kiosk_Location_"."KioskOwner_ID" = "Kiosk_Location_KioskOwner_"."KioskOwner_ID"
	left join "dbo"."M_Material" "Mater_" on "M_Kiosk_Material"."Mater_ID" = "Mater_"."Mater_ID"
	left join "dbo"."M_Material_Type" "Mater_MaterType_" on "Mater_"."MaterType_Code" = "Mater_MaterType_"."MaterType_Code"]]></text>
    </command>
  </commands>
